  "description": "Sidecar for rotating log files to objectstore",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:live": "NODE_OPTIONS='--expose-gc' nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
